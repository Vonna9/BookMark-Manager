{
  "name": "CalHacks Bookmark Manager",
  "description": "by Yvonna, Patrick, Mike, and Valentine",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/logo.png"
  },
  "permissions": ["bookmarks", "contextMenus"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "js/main.js"
  }
}
